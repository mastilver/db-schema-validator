{
  "name": "db-schema-validator",
  "version": "0.3.0",
  "description": "My bee&#39;s knees module",
  "license": "MIT",
  "repository": "mastilver/db-schema-validator",
  "author": {
    "name": "Thomas Sileghem",
    "email": "tsileghem@neworbit.co.uk",
    "url": "mastilver.com"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "xo && ava --require babel-core/register",
    "prebuild": "rimraf dist",
    "build": "babel lib --out-dir dist",
    "prepublish": "npm run build"
  },
  "files": [
    "index.js",
    "dist"
  ],
  "keywords": [
    ""
  ],
  "dependencies": {
    "is-integer": "^1.0.6"
  },
  "devDependencies": {
    "ava": "^0.10.0",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-3": "^6.3.13",
    "rimraf": "^2.5.0",
    "xo": "^0.12.1"
  },
  "xo": {
    "space": 4,
    "rules": {
      "brace-style": [
        2,
        "stroustrup",
        {
          "allowSingleLine": false
        }
      ]
    }
  },
  "ava": {
    "require": "babel-core/register"
  }
}
